{
  "name": "a-edge-extends-types",
  "type": "module",
  "version": "0.0.7",
  "main": "index.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "author": "lmssee  <lmssee@outlook.com>  (https://lmssee.com)",
  "description": "some (not all) edge extends types",
  "scripts": {
    "build": "npx ixxx rm dist && npm run b",
    "b": "rollup --config rollup.config.js && tsc -p tsconfig.types.json",
    "eslint": "ixxx cls && eslint src",
    "beautify": "npm run prettier",
    "prettier": "ixxx cls && prettier . --write",
    "versionPatch": "npm version patch --force --no-git-tag-version --allow-same-version=true",
    "only-up": "cd dist && npm publish && cd ../",
    "up": "npm run versionPatch && npm run build && npm run only-up"
  },
  "exports": {
    ".": {
      "default": "./index.js",
      "types": "./index.d.ts"
    },
    "./tab": "./src/tabs.d.ts",
    "./i18n": "./src/i18n.d.ts",
    "./action": "./src/action.d.ts",
    "./runtime": "./src/runtime.d.ts",
    "./storage": "./src/storage.d.ts",
    "./contextMenus": "./src/contextMenus.d.ts",
    "./tts": "./src/tts.d.ts"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "src/"
  ],
  "keywords": [
    "edge-extends-types",
    "rollup-plugin"
  ],
  "license": "ISC",
  "homepage": "https://lmssee.com/edge-extends-types",
  "bugs": {
    "url": "https://github.com/lmssee/edge-extends-types/issues",
    "email": "lmssee@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmssee/edge-extends-types.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/rollup-darwin-arm64": "^4.21.3",
    "@types/node": "^20.14.11",
    "@types/rollup": "^0.51.4",
    "eslint": "^8.57.0",
    "globals": "^15.8.0",
    "javascript-obfuscator": "^4.1.1",
    "prettier": "3.3.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-obfuscator": "^1.1.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1"
  }
}
